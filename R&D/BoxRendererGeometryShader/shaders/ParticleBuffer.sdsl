shader ParticleBuffer
{
    rgroup PerMaterial
    {
        stage StructuredBuffer<Particle> Particles;
    }

     struct Particle{
        float3 Velocity;
        float3 Position;
        float4 Color;
        float Drag;
        int Cycle;
        float Age;
        float LifeTime;
        int Id;
        float3 AxisY;
        float3 AxisX;
        float3 AxisZ;
        float3 Size3;

    };
};