shader SDFVertex
{
    struct Vertex
    {
        float3 position;
        float3 normal;
    };

    Vertex CreateVertex(float3 position = 0.0f, float3 normal = 0.0f, uint materialIndex = 0)
    {
        Vertex vertex;
        vertex.position = position;
        vertex.normal = normal;

        return vertex;
    }
};